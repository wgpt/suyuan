page .container {
  padding: 0;
}
.top-icon {
  width: 100%;
  display: flex;
  padding-bottom: 30rpx;
}
.top-icon navigator {
  display: flex;
  flex-direction: column;
  align-items: center;
  flex: 1;
  text-align: center;
}
.top-icon navigator image {
  width: 95rpx;
  height: 95rpx;
  margin: 40rpx 0 20rpx 0;
}
.top-icon navigator text {
  color: #101013;
  font-size: 30rpx;
}
.ah {
  width: 100%;
  height: 8px;
  background-color: #f1f1f1;
}
.ngt {
  width: 100%;
  box-sizing: border-box;
}
.ngt navigator,
.ngt .title {
  display: flex;
  height: 77rpx;
  line-height: 77rpx;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #f1f1f1;
}
.ngt navigator:last-of-type,
.ngt .title:last-of-type {
  border-bottom: none;
}
.ngt navigator image,
.ngt .title image {
  width: 17rpx;
  height: 30rpx;
  margin-right: 20rpx;
}
.ngt navigator text,
.ngt .title text {
  color: #101013;
  font-size: 32rpx;
  text-indent: 50rpx;
}
